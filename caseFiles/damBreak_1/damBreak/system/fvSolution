solvers
{
    "alpha.water.*"
    {
        nAlphaCorr      2;
        nAlphaSubCycles 1;
        cAlpha          1;

        MULESCorr       yes;
        nLimiterIter    5;

        solver          smoothSolver;
        smoother        symGaussSeidel;
        tolerance       1e-8;
        relTol          0;
    }
    
     "alpha.*"
    {
	interfaceMethod "isoAdvector";
	isoFaceTol	1e-10;
	surfCellTol	1e-3;
	snapTol		0;
	nAlphaBounds	3;
	clip		false;
	writeIsoFaces	false;
//	gradAlphaNormal	true;	

	nAlphaCorr	1;	
        nAlphaSubCycles 1;
        cAlpha          1;
        }

    "pcorr.*"
    {
        solver          PCG;
        preconditioner  DIC;
        tolerance       1e-5;
        relTol          0;
    }

    p_rgh
    {
        solver          PCG;
        preconditioner  DIC;
        tolerance       1e-07;
        relTol          0.05;
    }

    p_rghFinal
    {
        $p_rgh;
        relTol          0;
    }

    U
    {
        solver          smoothSolver;
        smoother        symGaussSeidel;
        tolerance       1e-06;
        relTol          0;
    }
    
    UFinal
    {
        $U;
        tolerance       1e-16;
        relTol          0;
    }
    
    T
    {
        solver          PBiCG;
        preconditioner  DILU;
        tolerance       1e-12;
        minIter         2;
        maxIter         50;//2
    }

    TFinal
    {
        $T;
    }
    
    "Y|Yexp"
    {
	$U;
    }

    Yi 
    {
	$U;
	relTol	0;
    }


    Phi
    {
    	$p_rgh;
    } 
    
}
